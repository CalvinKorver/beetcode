# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Crawler Configuration
START_URL=https://leetcode.com/problems/add-two-numbers/
MAX_PROBLEMS=5
RATE_LIMIT_MS=2000

# Optional: Headless mode (true/false)
HEADLESS=true

# Optional: Skip problems that already exist in database (true/false)
# Set to false to re-crawl and update existing problems
SKIP_EXISTING=true

# Authentication (for Premium problems)
# Path to cookies JSON file exported from browser after logging into LeetCode
# Leave empty to crawl without authentication (free problems only)
# To get cookies: Login to leetcode.com → F12 → Application → Cookies → Export
AUTH_COOKIES_FILE=
